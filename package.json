{
	"name": "auto-dark-mode-windows",
	"displayName": "Light/Dark Theme Switcher",
	"description": "Command to toggle theme between light/dark.",
	"repository": {
		"type": "git",
		"url": "https://github.com/danielgjackson/vscode-auto-dark-mode-windows.git"
	},
	"publisher": "danielgjackson",
	"version": "1.0.7",
	"license": "MIT",
	"engines": {
		"vscode": "^1.40.0"
	},
	"icon": "icon.png",
	"keywords": [
		"switch",
		"dark",
		"light",
		"theme",
		"dark mode"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:auto-dark-mode-windows.toggle"
	],
	"extensionKind": [
		"ui"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "auto-dark-mode-windows.toggle",
				"title": "Toggle Theme"
			}
		],
		"keybindings": [
			{
				"command": "auto-dark-mode-windows.toggle",
				"key": "ctrl+alt+shift+t",
				"mac": "cmd+alt+shift+t"
			}
		],
		"configuration": {
			"title": "Toggle Dark/Light Theme"
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test",
		"vscode:prepublish": "build.cmd"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
